alias ..='cd ..'
alias ...='cd ../..'
alias cd..='cd ..'
alias cd...='cd ../..'
alias finds='findstring.sh -i'
alias gitclone='git clone --recursive'
alias gits='git status --short'
alias gitupdate='git pull --stat --recurse-submodules=yes'
alias giturl='git config --get remote.origin.url'
alias h='history'
alias l='ls -Ahlp --color=auto'
alias rmmr='rmmacres.sh --dsstore --forks'
alias rmxattr='xattr -cr'
alias rp='realpath'
